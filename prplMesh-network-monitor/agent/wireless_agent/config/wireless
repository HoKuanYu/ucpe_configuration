
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option path 'platform/soc/a000000.wifi'
	option htmode 'HT20'
	option country 'TW'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option encryption 'none'
	option ieee80211v '1'
	option ifname 'wlan0'
	option macaddr '94:83:C4:0D:F1:25'
	option ssid 'prplmesh'

config wifi-device 'radio1'
	option type 'mac80211'
	option channel '36'
	option hwmode '11a'
	option path 'platform/soc/a800000.wifi'
	option htmode 'VHT80'
	option country 'TW'

config wifi-iface 'default_radio1'
	option device 'radio1'
	option network 'lan'
	option mode 'ap'
	option encryption 'none'
	option macaddr '94:83:C4:0D:F1:15'
	option ifname 'wlan1'
	option ssid 'prplmesh-5g'

config wifi-iface 'wifinet2'
	option ifname 'wlan0-1'
	option wds '1'
	option ssid 'prplWrt2'
	option encryption 'none'
	option device 'radio1'
	option mode 'sta'
	option bssid '94:83:C4:0D:F0:20'
	option macaddr '94:83:C4:0D:F1:35'
	option network 'lan'

config wifi-iface 'wifinet3'
	option wds '1'
	option encryption 'none'
	option device 'radio0'
	option mode 'sta'
	option bssid '94:83:C4:0D:F2:24'
	option macaddr '94:83:C4:0D:F1:30'
	option network 'lan'
	option ssid 'prplWrt2'
	option ifname 'wlan0-1'
	option disabled '1'

config wifi-iface 'wifinet4'
	option wds '1'
	option ssid 'prplWrt2'
	option encryption 'none'
	option device 'radio1'
	option mode 'sta'
	option network 'lan'
	option bssid '94:83:C4:0D:F2:45'
	option ifname 'wlan1-1'
	option disabled '1'

config wifi-iface 'wifinet5'
	option wds '1'
	option ssid 'prplWrt3'
	option encryption 'none'
	option device 'radio1'
	option mode 'ap'
	option network 'lan'
        option macaddr '94:83:C4:0D:F1:80'
        option ifname 'wlan1-2'

