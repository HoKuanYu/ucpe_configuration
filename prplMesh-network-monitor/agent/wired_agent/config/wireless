
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option path 'platform/soc/a000000.wifi'
	option htmode 'HT20'
	option country 'TW'
	option log_level '1'
	option disabled '1'

config wifi-device 'radio1'
	option type 'mac80211'
	option hwmode '11a'
	option path 'platform/soc/a800000.wifi'
	option country 'TW'
	option htmode 'VHT80'
	option channel '36'
	option log_level '1'
	option disabled '1'

config wifi-iface 'default_radio1'
	option device 'radio1'
	option network 'lan'
	option macaddr '94:83:C4:0D:F0:15'
	option key 'prplmesh_pass'
	option encryption 'none'
	option ieee80211v '1'
	option bss_transition '1'
	option mode 'ap'
	option ssid 'prplmesh-5g'
	option disabled '1'

config wifi-iface 'default_radio10'
	option device 'radio0'
	option network 'lan'
	option macaddr '94:83:C4:0D:F0:25'
	option key 'prplmesh_pass'
	option encryption 'none'
	option ssid 'prplmesh'
	option ieee80211v '1'
	option bss_transition '1'
	option mode 'ap'
	option wds '1'
	option disabled '1'

config wifi-iface 'wifinet4'
	option wds '1'
	option encryption 'none'
	option device 'radio1'
	option mode 'ap'
	option network 'lan'
	option macaddr '94:83:C4:0D:F0:20'
	option ssid 'prplWrt2'
	option disabled '1'

config wifi-iface 'wifinet5'
	option device 'radio1'
	option mode 'ap'
	option network 'lan'
	option macaddr '94:83:C4:0D:F0:30'
	option disassoc_low_ack '0'
	option wds '1'
	option key 'test_passphrase'
	option encryption 'none'
	option disabled '1'

